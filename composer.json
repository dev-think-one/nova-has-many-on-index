{
    "name": "yaroslawww/nova-has-many-on-index",
    "description": "A Laravel Nova field used to display a 'Has Many' relationship on the index screen.",
    "keywords": [
        "laravel",
        "nova-field",
        "has-many"
    ],
    "homepage": "https://github.com/yaroslawww/nova-has-many-on-index",
    "license": "MIT",
    "authors": [
        {
            "name": "Yaroslav Georgitsa",
            "email": "yaroslav.georgitsa@gmail.com",
            "homepage": "https://github.com/yaroslawww",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "NovaHasManyIndex\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaHasManyIndex\\FieldServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
